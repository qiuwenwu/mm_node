{
    "name": "stock_info2",
    "title": "股票信息",
    "table": "stock_info",
    "key": "stock_id",
    "orderby_default": "`stock_id` desc",
    "field_obj": "`stock_id`,`name`,`code`,`company`,`description`,`bonus`,`issue_price`,`pe`,`pb`,`profit`,`num`,`time_to_market`",
    "field_default": "`stock_id`,`name`,`code`,`company`,`description`,`bonus`,`issue_price`,`pe`,`pb`,`profit`,`num`,`time_to_market`",
    "query": {
        "name": "`name` like '%{0}%'",
        "code": "`code` like '%{0}%'",
        "company": "`company` like '%{0}%'",
        "description": "`description` like '%{0}%'",
        "bonus_min": "`bonus` >= {0}",
        "bonus_max": "`bonus` <= {0}",
        "issue_price_min": "`issue_price` >= {0}",
        "issue_price_max": "`issue_price` <= {0}",
        "pe_min": "`pe` >= {0}",
        "pe_max": "`pe` <= {0}",
        "pb_min": "`pb` >= {0}",
        "pb_max": "`pb` <= {0}",
        "profit_min": "`profit` >= {0}",
        "profit_max": "`profit` <= {0}",
        "num_min": "`num` >= {0}",
        "num_max": "`num` <= {0}",
        "time_to_market_min": "`time_to_market` >= '{0}'",
        "time_to_market_max": "`time_to_market` <= '{0}'",
        "keyword": "(`name` like '%{0}%' || `description` like '%{0}%')"
    },
    "update": {
        "bonus_add": "`bonus` = `bonus` + {0}",
        "issue_price_add": "`issue_price` = `issue_price` + {0}",
        "pe_add": "`pe` = `pe` + {0}",
        "pb_add": "`pb` = `pb` + {0}",
        "profit_add": "`profit` = `profit` + {0}",
        "num_add": "`num` = `num` + {0}"
    },
    "field_hide": []
}